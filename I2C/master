    %assign/v0 v0175BFE8_0, 0, 8;
    %delay 600000, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v006034A8_0, 0, 1;
    %jmp T_3.6;
T_3.1 ;
    %ix/getv/s 1, v01734268_0;
    %jmp/1 T_3.7, 4;
    %load/x1p 8, v017365A8_0, 1;
    %jmp T_3.8;
T_3.7 ;
    %mov 8, 2, 1;
T_3.8 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %assign/v0 v01736498_0, 0, 8;
    %load/v 8, v01734268_0, 32;
    %cmpi/u 8, 0, 32;
    %jmp/0xz  T_3.9, 4;
    %vpi_call 3 139 "$display", "Address %g bit sent =%b", v01734268_0, &PV<v017365A8_0, v01734268_0, 1>;
    %vpi_call 3 140 "$display", "Address sent=%b", v017365A8_0;
    %movi 8, 2, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v0175BFE8_0, 0, 8;
    %jmp T_3.10;
T_3.9 ;
    %vpi_call 3 145 "$display", "Address %g bit sent=%b", v01734268_0, &PV<v017365A8_0, v01734268_0, 1>;
    %load/v 8, v01734268_0, 32;
    %mov 40, 39, 1;
    %subi 8, 1, 33;
    %set/v v01734268_0, 8, 32;
T_3.10 ;
    %jmp T_3.6;
T_3.2 ;
    %set/v v006033F8_0, 0, 1;
    %load/v 8, v017208D8_0, 1;
    %set/v v0060C528_0, 8, 1;
