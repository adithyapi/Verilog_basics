    %jmp T_3.6;
T_3.3 ;
    %load/v 8, v017342C0_0, 32;
    %cmp/s 0, 8, 32;
    %jmp/0xz  T_3.11, 5;
    %set/v v006033F8_0, 1, 1;
    %ix/getv/s 1, v017342C0_0;
    %jmp/1 T_3.13, 4;
    %load/x1p 8, v01734210_0, 1;
    %jmp T_3.14;
T_3.13 ;
    %mov 8, 2, 1;
T_3.14 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %assign/v0 v01736498_0, 0, 8;
    %load/v 8, v017342C0_0, 32;
    %mov 40, 39, 1;
