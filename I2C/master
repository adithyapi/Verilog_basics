    %vpi_call 3 167 "$display", "data acq---time=%g, %g", $time, v017342C0_0;
    %jmp T_3.12;
T_3.11 ;
    %vpi_call 3 171 "$display", "state=%g, time=%g", v0175BFE8_0, $time;
    %movi 8, 4, 32;
    %ix/load 0, 32, 0;
