    .scope S_00609ED8;
T_0 ;
    %vpi_call 3 33 "$display", "MASTER COMMUNICATING";
    %movi 8, 7, 32;
    %set/v v01734268_0, 8, 32;
    %movi 8, 7, 32;
    %set/v v017342C0_0, 8, 32;
    %movi 8, 160, 8;
    %set/v v01734210_0, 8, 8;
    %movi 8, 162, 8;
    %set/v v017365A8_0, 8, 8;
    %set/v v01736600_0, 0, 1;
    %set/v v0175BFE8_0, 0, 32;
    %set/v v006034A8_0, 0, 1;
    %set/v v006033F8_0, 1, 1;
    %set/v v01736EB8_0, 1, 1;
    %set/v v01736498_0, 1, 1;
    %end;
    .thread T_0;
    .scope S_00609ED8;
T_1 ;
    %wait E_01735940;
    %load/v 8, v01736EB8_0, 1;
    %inv 8, 1;
    %set/v v01736EB8_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_00609ED8;
T_2 ;
    %wait E_01735960;
T_2.0 ;
    %load/v 8, v006034A8_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz T_2.1, 4;
    %set/v v01736600_0, 1, 1;
    %delay 1300000, 0;
    %set/v v01736600_0, 0, 1;
    %delay 1200000, 0;
    %jmp T_2.0;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_00609ED8;
T_3 ;
    %wait E_01735980;
    %load/v 8, v0175BFE8_0, 32;
    %cmpi/u 8, 0, 32;
    %jmp/1 T_3.0, 6;
    %cmpi/u 8, 1, 32;
    %jmp/1 T_3.1, 6;
    %cmpi/u 8, 2, 32;
    %jmp/1 T_3.2, 6;
    %cmpi/u 8, 3, 32;
    %jmp/1 T_3.3, 6;
    %cmpi/u 8, 4, 32;
    %jmp/1 T_3.4, 6;
    %cmpi/u 8, 5, 32;
    %jmp/1 T_3.5, 6;
    %jmp T_3.6;
T_3.0 ;
    %ix/load 0, 1, 0;
    %assign/v0 v01736498_0, 0, 0;
    %vpi_call 3 127 "$display", "START MASTER---state=%g, time=%g", v0175BFE8_0, $time;
